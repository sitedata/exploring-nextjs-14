    "crypto": "^1.0.1",
    "express": "^4.18.2",
    "next": "^14.0.2",
    "next-connect": "^1.0.0-next.4",
    "parse": "^4.3.1",
    "parse-server": "^6.4.0",
    "postcss": "8.4.31",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "^4.12.0",
    "react-wrap-balancer": "^1.1.0",
    "tailwind-merge": "^2.2.0",
